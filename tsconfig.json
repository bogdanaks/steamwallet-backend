{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ES2020",
    ],
    "module": "CommonJS",
    "skipLibCheck": true,
    "baseUrl": "./src",
    "allowImportingTsExtensions": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "src"
  ],
}